	"keywords" : ["php", "luya", "module", "admin"],
	"license": "MIT",
	"homepage" : "http://luya.io",
	"minimum-stability": "dev",
	"authors" : [
		{
			"name" : "Basil",
			"email" : "git@nadar.io",
			"homepage" : "https://github.com/nadar"
